'' 10 = 1010
'' bits 1 and
'' 10*x = 2*(5*x) = 2*(x+4*x)
pub times1(x)
  return x*1
  
pub times3(x)
  return x*3

pub times4(x)
  return x*4

pub times5(x)
  return x*5
  
pub times6(x)
  return x*6
  
pub times7(x)
  return x*7

pub times9(x)
  return 9*x

pub times10(x)
  return 10*x

pub times11(x)
  return 11*x

pub times12(x)
  return 12*x

pub times15(x)
  return x*15

